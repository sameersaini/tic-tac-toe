{
  "name": "react-tic-tac-toe",
  "version": "2.1.2",
  "private": true,
  "dependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "react": "^16.4.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5",
